{
  "rules": {
    "users": {
      ".read": "auth != null",
      "$user_id": {
        ".write": "auth != null && $user_id === auth.uid",
      }
    },
    "usersData": {
      "$user_id": {
        ".read": "auth != null && $user_id === auth.uid",
        ".write": "auth != null && $user_id === auth.uid",
        "displayName": {
          ".read": "auth != null"
        },
        "photoUrl": {
          ".read": "auth != null"
        }
      }
    }
  }
}